{
    "name": "lit-sandbox",
    "version": "0.0.1",
    "description": "A lit-element sandbox",
    "main": "index.js",
    "dependencies": {
        "@types/uuid": "^3.4.7",
        "@webcomponents/webcomponentsjs": "^2.4.1",
        "uuid": "^3.4.0"
    },
    "devDependencies": {
        "@types/webpack": "^4.41.5",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@open-wc/karma-esm": "^2.13.4",
        "@open-wc/testing": "^2.5.2",
        "@open-wc/testing-karma": "^3.2.37",
        "@types/chai": "^4.2.8",
        "@types/mocha": "^7.0.1",
        "@types/sinon": "^7.5.1",
        "@typescript-eslint/eslint-plugin": "^2.19.0",
        "@typescript-eslint/parser": "^2.19.0",
        "chai": "^4.2.0",
        "copy-webpack-plugin": "^5.1.1",
        "cross-env": "^7.0.0",
        "css-loader": "^3.4.2",
        "cssnano": "^4.1.10",
        "dart-sass": "^1.25.0",
        "deepmerge": "^4.2.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-lit": "^1.2.0",
        "eslint-plugin-prettier": "^3.1.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "karma": "^4.4.1",
        "lit-element": "^2.2.1",
        "mocha": "^7.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^1.19.1",
        "rimraf": "^3.0.1",
        "sass-loader": "^8.0.2",
        "sinon": "^8.1.1",
        "ts-loader": "^6.2.1",
        "typescript": "^3.7.5",
        "webpack": "^4.41.5",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.2"
    },
    "scripts": {
        "build": "npm run clean && npm run build:webpack",
        "start": "npm run clean && npm run start:webpack",
        "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config/prod.js",
        "start:webpack": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config/dev.js",
        "analyze": "WEBPACK_ANALYZER=true npm run build:webpack",
        "lint": "eslint \"**/*.+(js|ts)\"",
        "lint:fix": "npm run lint -- --fix",
        "clean": "rimraf \"dist\"",
        "test": "karma start --coverage",
        "test:watch": "karma start --auto-watch=true --single-run=false"
    },
    "keywords": [
        "lit-element",
        "typescript",
        "web-components",
        "webpack"
    ],
    "author": "jasonhulbert@github",
    "license": "ISC"
}
